{
	"name": "n8n-mcp-server",
	"version": "0.1.0",
	"description": "MCP server for building and managing n8n workflows using FastMCP",
	"main": "dist/index.js",
	"type": "module",
	"private": true,
	"bin": {
		"n8n-mcp-server": "dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc && shx chmod +x dist/index.js",
		"watch": "tsc --watch",
		"start": "node dist/index.js",
		"dev": "npx fastmcp dev src/index.ts",
		"inspect": "npx fastmcp inspect src/index.ts",
		"publish-packages": "pnpm run build && changeset publish"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"dedent": "^1.7.0",
		"fastmcp": "^3.16.0",
		"zod": "^4.1.5",
		"uuid": "^9.0.1",
		"@types/uuid": "^10.0.0",
		"node-fetch": "^2.6.7",
		"ajv": "^8.12.0",
		"ajv-formats": "^3.0.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@types/node": "^24.3.1",
		"shx": "^0.4.0",
		"typescript": "^5.9.2"
	}
}
